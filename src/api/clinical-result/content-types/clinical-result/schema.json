{
  "kind": "collectionType",
  "collectionName": "clinical_results",
  "info": {
    "singularName": "clinical-result",
    "pluralName": "clinical-results",
    "displayName": "Clinical Result"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "Description": {
      "type": "customField",
      "options": {
        "preset": "toolbarBaloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "Gallery": {
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ],
      "type": "media",
      "multiple": true
    },
    "PostedOn": {
      "type": "date"
    }
  }
}
